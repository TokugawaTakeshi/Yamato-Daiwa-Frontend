provideCallableExpressionSchemeAndRelatedComponentStyles()

  .CallableExpressionScheme

    &-TechnicalNameRow,
    &-ClosingBracket

      font-family Consolas, monaco, Meiryo UI, monospace
      MultilineTextWithoutExtraSpaceCausedByLineHeight({ fontSize: 1.2rem, lineHeight: 1.4rem })

      font-weight bold
      color ShadesOfGray__39ColorsW3C_Palette[12]


    &-LocalizedDescriptiveHeading

      Paddings({ horizontalSymmetric: 1rem })
      MultiLineTextElementOverflowSafetyWithoutExtraSpaceCausedByLineHeight({ fontSize: 1rem, lineHeight: 1.2rem })
      color ShadesOfGray__39ColorsW3C_Palette[20]

      margin-top 0.2rem


    &-GridFlow

      display grid
      grid-template-columns repeat(auto-fill, minmax(280px, 1fr));
      gap 6px

      Paddings({ horizontalSymmetric: 8px })

      PositionalRelationship({
        definitions:\
            (".CallableExpressionScheme-FunctionNameRow" { retireFromIt: { y: 6px } })\
            (".CallableExpressionScheme-FunctionNameLocalizedInterpretation" { retireFromIt: { y: 8px } })
      })

    &-ClosingBracketRow

      display grid
      grid-template-columns auto minmax(0, 1fr)
      align-items baseline
      grid-column-gap 4px

      margin-top 6px


    &-ReturnedValue

      font-family Consolas, monaco, Meiryo UI, monospace
      font-weight bold
      color ShadeOfGray__12--W3C_Palette


    &-UsedTypesGridFlow

      display grid
      grid-template-columns repeat(auto-fill, minmax(280px, 1fr));
      gap 6px

      retireFrom({ targetElementSelector: ".CallableExpressionScheme-ClosingBracketRow", y: 8px })


  .FunctionParameterAbstract

    display grid
    grid-template-columns auto minmax(0, 1fr)
    grid-gap 6px 12px
    align-items start

    border-width 1px
    border-radius 3px
    padding 6px 10px

    border-style solid
    border-color: JQueryUI_BaseTheme.content.borderColor
    color: JQueryUI_BaseTheme.content.fontColor


    &-HeaderRow

      grid-column-start span 2

      display flex
      align-items center


    &-Title

      font-size 13px
      font-weight bold
      color: ShadesOfGray__39ColorsW3C_Palette[18]


    &-RequirementBadge

      margin-left 6px


    &-HeaderCell

      text-align right

      color: ShadesOfGray__39ColorsW3C_Palette[20]


    &-LocalizedName

      MultiLineTextElementOverflowSafetyWithoutExtraSpaceCausedByLineHeight({ fontSize: 1rem, lineHeight: 1.2rem })
      color: ShadesOfGray__39ColorsW3C_Palette[14]


    &-Type

      font-family Consolas, monaco, Meiryo UI, monospace
      MultiLineTextElementOverflowSafetyWithoutExtraSpaceCausedByLineHeight({ fontSize: 1rem, lineHeight: 1.2rem })
      font-weight bold
      color: YamatoDaiwaInformationDesignColors.importantEntity

