  },
  "module": "./Distributable/index.js",
  "types": "./Distributable/index.d.ts",
  "devDependencies": {
    "@yamato-daiwa/automation": "../../../../YamatoDaiwaAutomation/MainPackage",
    "@yamato-daiwa/es-extensions": "1.6.9",
    "@yamato-daiwa/frontend": "../../../CoreLibrary/Package",
    "@yamato-daiwa/style_guides": "0.0.57",
    "rimraf": "5.0.1",
    "typescript": "5.2.2",
    "vue": "3.3.4",
    "vue-facing-decorator": "3.0.2",
    "vue-router": "4.2.4",
    "vue3-slide-up-down": "2.0.1"
  },
  "peerDependencies": {
    "vue": "3.2.47",
    "@yamato-daiwa/frontend": "2.0.0-alpha.2"
  },
  "scripts": {
    "Rebuilding of distributable": "yda build --selectiveExecution Distributable --mode PRODUCTION & npm run \"TypeScript types generating\"",
    "TypeScript types generating": "tsc --project tsconfig.declarations.json --declaration --emitDeclarationOnly --outDir Distributable",
    "Tests incremental building": "rimraf Tests/Output & yda build --selectiveExecution Tests --mode LOCAL_DEVELOPMENT"
  }
