  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/TokugawaTakeshi/yamato_daiwa-frontend.git"
  },
  "bugs": {
    "url": "https://github.com/TokugawaTakeshi/yamato_daiwa-frontend/issues",
    "email": "tokugawa.takesi@gmail.com"
  },
  "module": "./Distributable/index.js",
  "types": "./Distributable/index.d.ts",
  "dependencies": {
    "@babel/types": "7.20.7",
    "@yamato-daiwa/es-extensions": "1.6.0-alpha.3",
    "@yamato-daiwa/frontend": "2.0.0-alpha.1",
    "vue": "3.2.45",
    "vue-class-component": "8.0.0-rc.1",
    "vue-property-decorator": "10.0.0-rc.3",
    "vue-router": "4.1.6",
    "vue3-slide-up-down": "1.2.5",
    "vuex-module-decorators": "2.0.0"
  },
  "devDependencies": {
    "@yamato-daiwa/automation": "../../../../YamatoDaiwaAutomation/MainPackage",
    "@yamato-daiwa/style_guides": "0.0.36",
    "rimraf": "3.0.2",
    "ts-node": "10.9.1",
    "typescript": "4.9.4"
  },
  "scripts": {
    "Rebuilding of distributable": "yda build --selectiveExecution Distributable --mode PRODUCTION",
    "TypeScript types generating": "tsc --project tsconfig.declarations.json --declaration --emitDeclarationOnly --outDir Distributable",
    "Tests incremental building": "rimraf Tests/Output & yda build --selectiveExecution Tests --mode LOCAL_DEVELOPMENT"
  }
}
