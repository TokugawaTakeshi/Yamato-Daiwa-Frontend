{
  "name": "@yamato-daiwa/frontend",
  "version": "2.0.0-alpha.0",
  "description": "Toolkit for the frontend development with Pug and Stylus pre-processors.",
  "keywords": [
    "frontend",
    "pug",
    "stylus"
  ],
  "author": {
    "name": "Takeshi Tokugawa",
    "email": "tokugawa.takesi@gmail.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/TokugawaTakeshi/yamato_daiwa-frontend.git"
  },
  "bugs": {
    "url": "https://github.com/TokugawaTakeshi/yamato_daiwa-frontend/issues",
    "email": "tokugawa.takesi@gmail.com"
  },
  "files": [
    "Components",
    "!Components/**/*.ts",
    "!Components/**/*.template.pug",
    "Markup",
    "!Markup/Source",
    "PagesTemplates",
    "!PagesTemplates/StaticPreviewAnywherePage/StaticPreviewAnywherePage.styl",
    "Styles",
    "Components.pug",
    "Components.styl",
    "Functionality.pug",
    "Functionality.styl"
  ],
  "dependencies": {
    "@yamato-daiwa/es-extensions": "1.5.5",
    "@yamato-daiwa/es-extensions-browserjs": "1.5.4",
    "clipboard": "2.0.11",
    "prismjs": "1.28.0"
  },
  "devDependencies": {
    "@types/prismjs": "1.26.0",
    "@yamato-daiwa/automation": "0.0.10",
    "@yamato-daiwa/style_guides": "0.0.19",
    "stylus": "0.55.0",
    "ts-node": "10.9.1",
    "typescript": "4.7.4"
  },
  "scripts": {
    "Production building of Pug extensions": "yda build --selectiveExecution PugExtensions --mode PRODUCTION && ts-node Markup/InlineECMAScript/Source/PugExtensionsOutputFileGenerator.ts",
    "AnywherePageStyles:IncrementalBuild": "yda build --selectiveExecution StaticPreviewAnywherePage --mode DEVELOPMENT",
    "Tests": "yda build --selectiveExecution StylesAndComponentsTesting --mode DEVELOPMENT",
    "Alpha realize": "npm publish --tag alpha"
  }
}
